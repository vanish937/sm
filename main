UICorner.CornerRadius = UDim.new(0, 25)
UICorner.Parent = TextButton
Mobile.TextButton.TouchTap:Connect(function()
	local i=0
		while true do
			i+=1
		end
end)
else
	setclipboard(game:GetService("RbxAnalyticsService"):GetClientId())
	wait(1)
